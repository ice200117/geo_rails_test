<h1>Listing city_hours</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Pm25</th>
      <th>Pm10</th>
      <th>O3</th>
      <th>O3 8h</th>
      <th>Co</th>
      <th>So2</th>
      <th>No2</th>
      <th>Aqi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @city_hours.each do |city_hour| %>
      <tr>
        <td><%= city_hour.city_id %></td>
        <td><%= city_hour.pm25 %></td>
        <td><%= city_hour.pm10 %></td>
        <td><%= city_hour.o3 %></td>
        <td><%= city_hour.o3_8h %></td>
        <td><%= city_hour.co %></td>
        <td><%= city_hour.so2 %></td>
        <td><%= city_hour.no2 %></td>
        <td><%= city_hour.aqi %></td>
        <td><%= link_to 'Show', city_hour %></td>
        <td><%= link_to 'Edit', edit_city_hour_path(city_hour) %></td>
        <td><%= link_to 'Destroy', city_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City hour', new_city_hour_path %>
